<h1>予約フォーム</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3"> 
   <%= form_for(@reserve) do |f| %>
    <div><%= f.label :date %>
    <%= f.date_field :date %></div>

    <div><%= f.label :number %>
    <%= f.number_field :number %></div>

    <%= f.submit "予約する" %>
   <% end %>
  </div>
</div>
